<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.TesttMapper">

    <select id="selectEmpInfo" resultType="java.util.Map">
        SELECT EMPNO
             , ENAME
             , JOB
             , MGR
             , HIREDATE
             , SAL
             , COMM
             , DEPTNO
        FROM EMP
        WHERE EMPNO = '7369'
    </select>
</mapper>
